{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "dotenv -e .env -- nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev": "dotenv -e .env.development -- nest start --watch",
    "start:prod": "dotenv -e .env -- node dist/main",
    "start:product": "yarn run migrate:deploy && yarn run start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "dotenv -e .env.development npx prisma migrate dev --preview-feature",
    "migrate:dev:create": "dotenv -e .env.development npx prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "dotenv -e .env.development npx prisma migrate reset --preview-feature",
    "migrate:dev:deploy": "dotenv -e .env.development npx prisma migrate deploy --preview-feature",
    "migrate:status": "dotenv -e .env.development npx prisma migrate status --preview-feature",
    "migrate:resolve": "dotenv -e .env.development npx prisma migrate resolve --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "prisma:ci:setup": "dotenv -e .env.ci -- npx prisma db push --accept-data-loss",
    "prisma:localci:setup": "dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "test": "npx prisma generate && dotenv -e .env.test npx prisma migrate deploy --preview-feature && dotenv -e .env.test jest",
    "ci:test": "dotenv -e .env.ci jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "start:migrate:prod": "yarn run migrate:deploy && yarn run start:prod",
    "seed": "prisma db seed",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.359.0",
    "@aws-sdk/s3-request-presigner": "^3.359.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^10.0.2",
    "@prisma/client": "^4.12.0",
    "@types/crypto-js": "^4.1.2",
    "@types/lodash": "^4.14.198",
    "api": "file:",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^3.6.0",
    "dotenv-cli": "^7.2.1",
    "eslint-config-custom": "*",
    "eslint-plugin-prettier": "5.0.0",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.9.0",
    "g": "^2.0.1",
    "google-auth-library": "^8.7.0",
    "hbs": "^4.2.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "myinfo-connector-v4-nodejs": "^1.0.2",
    "mysql2": "^3.9.3",
    "nestjs-prisma": "^0.20.0",
    "nodemailer": "^6.9.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize-typescript": "^2.1.5",
    "tsconfig": "*",
    "twilio": "^4.10.0",
    "typeorm": "^0.3.20",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "jest": "29.5.0",
    "prettier": "^3.0.2",
    "prisma": "^4.12.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "dist",
      "**/*.js"
    ]
  }
}
